{% capture getfileextname %}{{ include.url | split:"/" | last }}{% endcapture %}{% capture getfileextext %}{{ getfileextname | split:"." | last }}{% endcapture %}{% capture getfileextext %}{% if getfileextname != "" and getfileextname != getfileextext %}{{ getfileextext }}{% endif %}{% endcapture %}{% capture getfileextlast %}{{ include.url | slice: -1, 1 }}{% endcapture %}{% capture getfileextname2 %}{{ include.url | absolute_url }}{% if getfileextlast != "/" and getfileextext == "" %}.html{% endif %}{% endcapture %}{{ getfileextname2 | replace: "/index.html","/" }}